{
  "name": "Make Moves",
  "short_name": "moves.io",
  "description": "Instantly get any founder's email.",
  "version": "2.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make Moves",
    "default_icon": "img/moves_icon.png"
  },
  "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
  ],
  "icons": { "16": "img/moves_icon.png",
           "48": "img/moves_icon.png",
          "128": "img/moves_icon.png" },
  "content_security_policy": "script-src 'self' https://parse.com/downloads/javascript/parse-1.3.1.min.js; object-src 'self'",
  "manifest_version": 2
}
